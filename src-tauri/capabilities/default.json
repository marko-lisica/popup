{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "allow-get-config",
    "allow-exit-with-code",
    "allow-resize-window-to-content",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:window:allow-set-size"
  ]
}
